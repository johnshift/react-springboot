#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/bin/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/sshpk@1.16.1/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules:/home/hanscem/johnshift/veils/node_modules:/home/hanscem/johnshift/node_modules:/home/hanscem/node_modules:/home/node_modules:/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/http-signature@1.3.6/node_modules/http-signature/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/http-signature@1.3.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/bin/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/sshpk@1.16.1/node_modules/sshpk/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/sshpk@1.16.1/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules:/home/hanscem/johnshift/veils/node_modules:/home/hanscem/johnshift/node_modules:/home/hanscem/node_modules:/home/node_modules:/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/http-signature@1.3.6/node_modules/http-signature/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/http-signature@1.3.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sshpk@1.16.1/node_modules/sshpk/bin/sshpk-sign" "$@"
else
  exec node  "$basedir/../../../../../sshpk@1.16.1/node_modules/sshpk/bin/sshpk-sign" "$@"
fi
