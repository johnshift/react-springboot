#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/extract-zip@2.0.1_supports-color@8.1.1/node_modules/extract-zip/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/extract-zip@2.0.1_supports-color@8.1.1/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules:/home/hanscem/johnshift/veils/node_modules:/home/hanscem/johnshift/node_modules:/home/hanscem/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/extract-zip@2.0.1_supports-color@8.1.1/node_modules/extract-zip/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/extract-zip@2.0.1_supports-color@8.1.1/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules/.pnpm/node_modules:/home/hanscem/johnshift/veils/e2e-test/node_modules:/home/hanscem/johnshift/veils/node_modules:/home/hanscem/johnshift/node_modules:/home/hanscem/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
