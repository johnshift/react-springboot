{
  "compilerOptions": {
    // "target": "ESNext",
    "target": "es5",  
    "useDefineForClassFields": true,
    // "module": "ESNext",
    "module": "commonjs",
    "lib": ["ESNext", "DOM", "ES6"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "types": ["cypress","@testing-library/cypress"],
    "skipLibCheck": true  
  },
  "include": ["./src", "./cypress"]
}
